{
  "name": "@riddance/aws-env",
  "version": "0.0.6",
  "type": "module",
  "keywords": [
    "riddance",
    "environment",
    "devops",
    "opinionated",
    "aws",
    "lambda"
  ],
  "license": "MIT",
  "engines": {
